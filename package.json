{
  "name": "@pixi/react",
  "version": "8.0.0",
  "description": "Write PixiJS applications using React declarative style.",
  "keywords": [
    "react",
    "pixi"
  ],
  "homepage": "https://github.com/pixijs/pixi-react#readme",
  "bugs": {
    "url": "https://github.com/pixijs/pixi-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixijs/pixi-react.git"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./lib/index.mjs"
      },
      "require": {
        "types": "./types/index.d.ts",
        "default": "./lib/index.js"
      }
    }
  },
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "dist/",
    "types/"
  ],
  "scripts": {
    "build": "xs build",
    "clean": "xs clean",
    "docs": "xs docs",
    "lint": "xs lint",
    "lint:fix": "xs lint --fix",
    "prepare": "husky install",
    "release": "xs release",
    "start": "xs serve",
    "test": "vitest run",
    "test:lint": "xs lint",
    "test:watch": "vitest",
    "types": "xs types",
    "watch": "xs watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,mjs}": [
      "eslint --cache --fix --max-warnings 0"
    ]
  },
  "dependencies": {
    "react-reconciler": "^0.29.2"
  },
  "devDependencies": {
    "@pixi/extension-scripts": "^2.4.1",
    "@types/react": "^18.3.2",
    "@types/react-reconciler": "^0.28.8",
    "copyfiles": "^2.4.1",
    "husky": "^8.0.0",
    "pixi.js": "^8.1.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "pixi.js": "^8.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "extensionConfig": {
    "lint": [
      "test",
      "src"
    ],
    "environments": ["node"],
    "docsName": "PixiJS React",
    "docsTitle": "PixiJS React API Documentation",
    "docsDescription": "Documentation for PixiJS React library",
    "docsKeyword": "docs, documentation, pixi, pixijs, react, html5, javascript, jsdoc"
  }
}
